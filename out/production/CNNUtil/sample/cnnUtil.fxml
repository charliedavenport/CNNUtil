<BorderPane prefHeight="600.0" prefWidth="800.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2" fx:controller="sample.Controller">
  <center>
    <TabPane prefHeight="200.0" prefWidth="200.0" tabClosingPolicy="UNAVAILABLE" tabMaxWidth="100.0" tabMinWidth="50.0">
      <tabs>
        <Tab text="Model">
          <content>
            <AnchorPane id="Content" minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
              <children>
                <ComboBox id="comboBox" fx:id="modelComboBox" layoutX="32.0" layoutY="27.0" prefHeight="21.0" prefWidth="130.0" promptText="Select Model">
                  <items>
                    <FXCollections fx:factory="observableArrayList" />
                  </items>
                </ComboBox>
                <VBox alignment="TOP_CENTER" layoutX="14.0" layoutY="69.0" prefHeight="462.0" prefWidth="386.0" style="-fx-background-color: White; ">
                  <padding>
                    <Insets />
                  </padding>
                  <Label alignment="CENTER" minHeight="21.0" prefHeight="34.0" prefWidth="386.0" style=" -fx-border-color:black; -fx-border-width: 1; -fx-border-style: solid;" text="Model Data" textAlignment="CENTER" underline="false">
                    <font>
                      <Font name="System Bold" size="16.0" fx:id="x1" />
                    </font>
                  </Label>
                  <Pane prefHeight="428.0" prefWidth="170.0" style=" -fx-border-color:black; -fx-border-width: 1; -fx-border-style: solid;">
                    <children>
                      <ListView id="listView" fx:id="modelDataListView" layoutX="0.0" layoutY="0.0" prefHeight="428.0" prefWidth="386.0" />
                    </children>
                  </Pane>
                </VBox>
              </children>
            </AnchorPane>
          </content>
        </Tab>
        <Tab text="Data">
          <content>
            <AnchorPane id="Content" minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
              <children>
                <ComboBox fx:id="datasetComboBox" layoutX="34.0" layoutY="29.0" onAction="#handleDatasetCombo" prefHeight="21.0" prefWidth="127.0" promptText="Select DataSet">
                  <items>
                    <FXCollections fx:factory="observableArrayList" />
                  </items>
                </ComboBox>
                <VBox alignment="TOP_CENTER" layoutX="34.0" layoutY="71.0" prefHeight="462.0" prefWidth="284.0" style="-fx-background-color: White; ">
                  <padding>
                    <Insets />
                  </padding>
                  <Label alignment="CENTER" font="$x1" minHeight="21.0" prefHeight="34.0" prefWidth="386.0" style=" -fx-border-color:black; -fx-border-width: 1; -fx-border-style: solid;" text="Dataset" textAlignment="CENTER" underline="false" />
                  <Pane prefHeight="428.0" prefWidth="170.0" style=" -fx-border-color:black; -fx-border-width: 1; -fx-border-style: solid;">
                    <children>
                      <ListView id="listView" layoutX="0.0" layoutY="0.0" prefHeight="428.0" prefWidth="284.0" />
                    </children>
                  </Pane>
                </VBox>
              </children>
            </AnchorPane>
          </content>
        </Tab>
        <Tab text="Train">
          <content>
            <AnchorPane id="Content" minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
          </content>
        </Tab>
        <Tab text="Evaluate">
          <content>
            <AnchorPane id="Content" minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
          </content>
        </Tab>
        <Tab text="Stats">
          <content>
            <AnchorPane id="Content" minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
          </content>
        </Tab>
      </tabs>
    </TabPane>
  </center>
  <padding>
    <Insets />
  </padding>
</BorderPane>
